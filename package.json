{
  "name": "ynab-api-starter-kit",
  "author": "You Need a Budget, LLC",
  "license": "Apache-2.0",
  "version": "1.1.3",
  "description": "To quickly get started building an App with the YNAB API.",
  "bin": "bin/start.js",
  "type": "module",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production --progress",
    "migrate:up": "ts-node src/db/migrate.ts up",
    "migrate:down": "ts-node src/db/migrate.ts down",
    "migrate:create": "node-pg-migrate create -j ts"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/pg": "^8.11.11",
    "chalk": "^5.3.0",
    "cpr": "^3.0.1",
    "exponential-backoff": "^3.1.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "joi": "^17.13.3",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "vue": "^3.4.5",
    "ynab": "^2.2.0",
    "@types/pg": "^8.10.9",
    "pino-pretty": "^10.3.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "css-loader": "^6.8.1",
    "vue-loader": "^17.4.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "@types/node": "^20.11.16",
    "typescript": "^5.3.3"
  }
}
